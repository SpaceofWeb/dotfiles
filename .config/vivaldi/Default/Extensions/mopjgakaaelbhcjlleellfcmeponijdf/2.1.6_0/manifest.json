{
   "background": {
      "persistent": false,
      "scripts": [ "assets/js/socket.io.js", "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup/popup.html",
      "default_title": "VK Music to Telegram"
   },
   "content_scripts": [ {
      "css": [ "assets/style.css" ],
      "js": [ "assets/js/axios.min.js", "assets/js/hls.js", "assets/js/jquery-3.4.1.slim.min.js", "content.js" ],
      "matches": [ "https://vk.com/*", "https://vkmt-server.now.sh/*" ],
      "run_at": "document_end"
   } ],
   "description": "Отправляет музыку из ВКонтакте в Телеграм",
   "homepage_url": "https://vkmt-server.now.sh",
   "icons": {
      "128": "assets/img/128x128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "VK Music to Telegram",
   "options_ui": {
      "page": "popup/popup.html"
   },
   "permissions": [ "activeTab", "tabs", "storage", "notifications" ],
   "short_name": "VKMT",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1.6",
   "web_accessible_resources": [ "assets/img/*" ]
}
